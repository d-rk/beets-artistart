    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/beet",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ],
            "env": {
              "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python Debugger: artistart",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/beet",
            "console": "integratedTerminal",
            "args": [
                // "import", "/home/dirk/Music/Import/Counting Crows/"
                "artistart", "Butter Miracle, Suite One"
            ],
            "env": {
              "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}